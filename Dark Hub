local FTab = Window:CreateTab("Tab F", 4483362458) -- Title, Image
local ITab = Window:CreateTab("Tab I", 4483362458) -- Title, Image
local JTab = Window:CreateTab("Tab J", 4483362458) -- Title, Image
local KTab = Window:CreateTab("Tab K", 4483362458) -- Title, Image
local LTab = Window:CreateTab("Tab L", 4483362458) -- Title, Image
local MTab = Window:CreateTab("Tab M", 4483362458) -- Title, Image
local NTab = Window:CreateTab("Tab N", 4483362458) -- Title, Image
