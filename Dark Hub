local DestoryButton = MainTab:CreateButton({
   Name = "Destroy Ui",
   Callback = function()
   Rayfield:Destroy()
   end,
})

local MainButton = MainTab:CreateButton({
   Name = "Discord Server Link",
   Callback = function()
      setclipboard("https://dsc.gg/darkhub")
   end,
})

local AriseCrossoverButton = AriseCrossoverTab:CreateButton({
   Name = "Twvz Hub - Keyless",
   Callback = function()
   loadstring(game:HttpGet("https://raw.githubusercontent.com/ZhangJunZ84/twvz/refs/heads/main/arisecrossover.lua"))()
   end,
})
